{
  "version": 2,
  "buildCommand": "npm install && cd backend && npm run build && cd ../frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/index.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "env": {
    "VERCEL": "1"
  }
}